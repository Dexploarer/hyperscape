{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Hyperscape API Documentation",
  "entryPointStrategy": "expand",
  "entryPoints": [
    "packages/shared/src/index.ts",
    "packages/client/src/index.tsx",
    "packages/server/src/index.ts"
  ],
  "out": "packages/docs-site/docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "outputFileStrategy": "modules",
  "fileExtension": ".md",
  "entryFileName": "index.md",
  "flattenOutputFiles": false,
  "membersWithOwnFile": ["Class", "Interface", "Enum"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "readme": "none",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/test-*.ts",
    "**/test-*.tsx",
    "**/scripts/**",
    "**/cypress/**",
    "**/__tests__/**",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/temp/**",
    "**/tmp/**"
  ],
  "excludeExternals": true,
  "excludeReferences": true,
  "excludeNotDocumented": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "disableSources": false,
  "commentStyle": "jsdoc",
  "categorizeByGroup": true,
  "externalSymbolLinkMappings": {
    "three": {
      "*": "https://threejs.org/docs/#api/en/{path}"
    }
  },
  "categoryOrder": [
    "Core",
    "Systems",
    "Entities",
    "Nodes",
    "Components",
    "Types",
    "Utilities",
    "*"
  ],
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "suppressCommentWarningsInDeclarationFiles": true,
  "logLevel": "Error",
  "tsconfig": "tsconfig.json",
  "githubPages": false,
  "hideGenerator": false,
  "navigationLinks": {
    "GitHub": "https://github.com/HyperscapeAI/hyperscape",
    "Documentation": "https://docs.hyperscape.xyz"
  }
}

