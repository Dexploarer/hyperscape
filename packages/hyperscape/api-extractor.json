{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  
  "projectFolder": ".",
  
  "mainEntryPointFilePath": "<projectFolder>/build/index.d.ts",
  
  "bundledPackages": [],
  
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.build.json",
    "skipLibCheck": true
  },
  
  "apiReport": {
    "enabled": false
  },
  
  "docModel": {
    "enabled": false
  },
  
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/build/hyperscape.d.ts",
    "betaTrimmedFilePath": "",
    "publicTrimmedFilePath": "",
    "omitTrimmingComments": true
  },
  
  "tsdocMetadata": {
    "enabled": false
  },
  
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-wrong-input-file-type": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
