{
  "name": "@hyperscape/hyperscape",
  "version": "0.13.0",
  "type": "module",
  "main": "build/framework.js",
  "types": "build/framework.d.ts",
  "bin": {
    "hyperscape": "build/cli.js"
  },
  "exports": {
    ".": {
      "types": "./build/framework.d.ts",
      "import": "./build/framework.js",
      "require": "./build/framework.js"
    }
  },
  "homepage": "https://github.com/hyperscape-xyz/hyperscape#readme",
  "bugs": {
    "url": "https://github.com/hyperscape-xyz/hyperscape/issues"
  },
  "keywords": [
    "hyperscape",
    "3d",
    "components",
    "virtual-worlds",
    "metaverse",
    "three.js",
    "webgl",
    "real-time"
  ],
  "scripts": {
    "dev": "bun scripts/build.mjs --dev",
    "build": "bun scripts/build.mjs",
    "build:client": "vite build",
    "build:server": "bun scripts/build.mjs --server-only",
    "start": "bun run build && bun build/index.js start --world ./world",
    "test": "vitest run --exclude src/tests/client-movement.test.ts",
    "test:client": "node test-client.mjs",
    "lint": "eslint src --max-warnings 22100"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^10.0.1",
